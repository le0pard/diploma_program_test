<h1>Редактировать страницу</h1>
<div class="admin-form">
<% form_for @page, :url => admin_page_path do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <div class="line submit">
    <%= f.submit 'Редактировать' %>
  </div>
<% end %>
</div>
<br />
<%= link_to "Назад", admin_page_index_path %>
